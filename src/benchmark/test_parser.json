  "synthesis_config": {
    "use_linear_invariant": true,
    "maximal_polynomial_degree": 1,
    "probability_threshold": 0.9999,
    "theorem_name": "farkas",
    "solver_name": "z3",
    "owl_path": "../../playground/rabinizer/owl"
  },

  "specification": {
    "ltl_formula": "G a",
    "proposition_lookup": {
      "a": "S1 <= 100"
    },
    "hoa_path": "./Ga.hoa"
  }
